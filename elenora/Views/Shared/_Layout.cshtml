+2212,U+2215,U+FEFF,U+FFFD}
    </style>
    <link rel="stylesheet" type="text/css" href="/photoswipe/photoswipe.css" />
    <link rel="stylesheet" type="text/css" href="/photoswipe/default-skin/default-skin.css" />
</head>
<body>
    @if ((bool)ViewData["AddTagManager"] == true)
    {
        <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NJR2MPC" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    }

    <script>
        window.fbAsyncInit = function () {
            FB.init({
                appId: '745047939582851',
                autoLogAppEvents: true,
                xfbml: true,
                version: 'v8.0'
            });
        };
    </script>
    <script async defer src="https://connect.facebook.net/hu_HU/sdk.js"></script>

    <div id="wrapper">
        <partial name="_CookiesPopup" />
        <partial name="_Header" />

        <main>
            @RenderBody()
        </main>

        <div id="overlay" onclick="hideCartDrawer(); hideSearchBar();"></div>

        <div id="popup-window">
            <div class="popup-window-overlay"></div>
            <div id="popup-window-content">
                <div id="popup-window-header">
                    <img id="popup-header-close" src="/images/close.svg" alt="Bezár" />
                </div>
                <div id="popup-window-content-inner"></div>
            </div>
        </div>
        <div id="loading">
            <img src="~/images/loading-white.gif" />
        </div>

        @if ((bool?)ViewData["CanShowPopup"] == true)
        {
            <div class="popup">
                @await Component.InvokeAsync("Popup", ViewData["CustomerId"])
            </div>
        }

        <partial name="_Footer" />
    </div>

    <div id="cart-drawer">
        @await Component.InvokeAsync("Cart", ViewData["CustomerId"])
    </div>

    <div id="image-preview">
        <div class="image-preview-overlay"></div>
        <div id="image-preview-content">
            <div id="image-preview-content-outer">
                <div id="image-preview-content-inner"></div>
            </div>
        </div>
    </div>

    <script type="text/javascript" src="/photoswipe/photoswipe.min.js"></script>
    <script type="text/javascript" src="/photoswipe/photoswipe-ui-default.min.js"></script>
    <partial name="_Photoswipe" />
</body>
</html>
