<!DOCTYPE html>
<html lang="en">
<head>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" type="text/javascript" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
    <script>
        window.jQuery || document.write('<script src="/jquery-3.4.1.min.js"><\/script>');
    </script>
    <script src="~/js/modernizr-webp.js"></script>

    @if ((bool)ViewData["AddTagManager"] == true)
    {
        <script>(function (w, d, s, l, i) { w[l] = w[l] || []; w[l].push({ 'gtm.start': new Date().getTime(), event: 'gtm.js' }); var f = d.getElementsByTagName(s)[0], j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f); })(window, document, 'script', 'dataLayer', 'GTM-NJR2MPC');</script>
    }

    <meta charset="utf-8" />
    <meta name="viewport" content="@(string.IsNullOrWhiteSpace((string)ViewData["MetaViewport"]) ? "width=device-width, initial-scale=1.0" : (string)ViewData["MetaViewport"])">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="p:domain_verify" content="69b2ec3f64e18d12d079376405e9f1eb"/>

    <title>@ViewData["Title"] @((bool?)ViewData["WithoutTitleSuffix"] == true ? "" : "- Elenora")</title>
    @if (ViewData["Description"] != null)
    {
        <meta name="description" content="@ViewData["Description"]" />
    }
    
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" type="text/css" />

    @if ((bool)ViewData["AddFbPixel"] == true)
    {
        <script src="//d2wy8f7a9ursnm.cloudfront.net/v7/bugsnag.min.js"></script>
        <script>Bugsnag.start({ apiKey: '478261cb2482da54678f5a2790689887' })</script>
    }

    @RenderSection("styles", false)

    @if ((bool)ViewData["AddFbPixel"] != true)
    {
        <script>
            fbq = function (a, b) {
                console.log(a + " " + b);
            }
        </script>
    }
    <script>
        var fbExternalId = '@ViewBag.FbExternalId';
    </script>

    @Html.Raw(ViewData["HeadTags"])

    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="/SiteJs.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)

    <style type="text/css">
